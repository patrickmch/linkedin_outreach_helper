{
  "name": "linkedin-outreach-helper",
  "version": "4.0.0",
  "description": "LinkedIn outreach automation with webhook bot, Redis storage, and AI-powered lead qualification",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "stats": "node batch-qualify.js",
    "send": "node batch-send.js",
    "sync": "node check-acceptances.js",
    "build": "tsc",
    "start:webhook": "node dist/webhook/server.js",
    "dev:webhook": "tsc && node --watch dist/webhook/server.js"
  },
  "keywords": [
    "linkedin",
    "outreach",
    "lead-qualification",
    "api",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@upstash/redis": "^1.35.6",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
